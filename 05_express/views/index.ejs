<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <script src="https://cdn.jsdelivr.net/npm/axios/dist/axios.min.js"></script>
    <title>로그인 하기</title>
  </head>
  <body>
    <h1>get axios로 로그인 하기</h1>
    <form name="register">
      <fieldset>
        <legend>LOGIN</legend>
        <label for="id">ID : <input type="text" name="id" id="id" /></label>
        <br />
        <label for="pw"
          >Password : <input type="text" name="pw" id="pw"
        /></label>
      </fieldset>
      <br />
      <button type="button" onclick="getAxios()">로그인 하기</button>
    </form>
    <script>
      async function getAxios() {
        const form = document.forms["register"];
        const data = {
          id: form.id.value,
          pw: form.pw.value,
        };
        try {
          response = await axios({
            method: "GET",
            url: "/axios",
            params: data,
          });
          console.log(response.data);
          const { id, pw } = response.data;
        } catch (err) {
          console.log("err!!", err);
        }
      }
    </script>
  </body>
</html>
